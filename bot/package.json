  "main": "./src/index.js",
  "scripts": {
    "dev": "set NODE_ENV=dev && nodemon --exec babel-node src/index.js -i ",
    "start": "nodemon --exec babel-node src/index.js -i ",
    "start:prod": "node --exec babel-node src/index.js -i ",
    "ngrok": "start cmd /k >ngrok ngrok http 8000 --bind-tls=true && nodemon --exec babel-node src/index.js -i ",
    "prod": "node ./lib/index.js",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build --ignore node_modules/**/*.js . -s",
    "build": "npm run clean && npm run build-server",
    "winbuild": "./node_modules/.bin/babel src -d lib",
    "deploy": "npm run winbuild && git add . && git commit && git push origin && git push heroku master",
    "postinstall": "npm run winbuild"
  },
